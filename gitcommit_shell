project_name=$1
echo "Start init ...."
git init
git add .
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:Leo-creator-3993/${project_name}.git
git remote set-url origin git@github.com:Leo-creator-3993/${project_name}.git
echo 'init finish!'
echo 'exec ==> git push -u origin main'
